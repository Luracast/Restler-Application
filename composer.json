{
    "name": "restler/application",
    "description": "Application boiler-plate for Luracast Restler framework",
    "keywords": [
        "restler",
        "rest",
        "api",
        "application"
    ],
    "minimum-stability": "dev",
    "license": "LGPL 2",
    "type": "project",
    "authors": [
        {
            "name": "Arul Kumaran",
            "email": "arul@luracast.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/database": "4.2.*",
        "illuminate/cache": "4.2.*",
        "illuminate/filesystem": "4.2.*",
        "luracast/config": "1.*",
        "vlucas/phpdotenv": "~1.0",
        "restler/framework": "3.0.0-RC6"
    },
    "require-dev": {
        "illuminate/console": "4.2.*",
        "d11wtq/boris": "~1.0"
    },
    "suggest": {
        "doctrine/dbal": "Allow renaming columns and dropping SQLite columns."
    },
    "autoload": {
        "classmap": [
            "app/commands",
            "app/models",
            "app/database/migrations",
            "app/database/seeds"
        ],
        "psr-0": {
            "": "app/controllers"
        },
        "psr-4": {
            "Bootstrap\\": "bootstrap/"
        }
    }
}
